# This is the virtual host configuration file for Apache, and should be
# copied to /etc/apache2/sites-available (depending on how Apache is insalled
# on your system) and enabled with the a2ensite command.
<VirtualHost *:80>
    ServerName penguicontrax.goldmoth.com

    WSGIDaemonProcess penguicontrax user=matt group=matt threads=5
    WSGIScriptAlias / /var/www/penguicontrax/venv/penguicontrax/penguicontrax.wsgi

    <Directory /var/www/penguicontrax/venv/penguicontrax>
        WSGIProcessGroup penguicontrax
        WSGIApplicationGroup %{GLOBAL}
        Order deny,allow
        Allow from all
    </Directory>
</VirtualHost>
